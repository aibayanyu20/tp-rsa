{
    "name": "aibayanyu/tp-rsa",
    "description": "this is thinkphp rsa",
    "license": "MIT",
    "authors": [
        {
            "name": "aibayanyu",
            "email": "aibayanyu@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "topthink/framework": "6.0.0",
        "aibayanyu/rsa-crypto": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "aibayanyu\\rsa": "src/"
        },
        "psr-0": {
            "file": "src/helpers.php"
        }
    },
    "type": "library",
    "version": "1.0.0",
    "extra": {
        "think": {
            "config": {
                "rsa": "src/config/config.php"
            },
            "services": [
                "aibayanyu\\rsa\\Service"
            ]
        }
    }
}
